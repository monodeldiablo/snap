<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkAssistant" id="import_assistant">
    <property name="border_width">12</property>
    <child>
      <object class="GtkLabel" id="import_path_label">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Import from...</property>
      </object>
      <packing>
        <property name="page_type">intro</property>
      </packing>
    </child>
    <child>
      <object class="GtkLabel" id="progress_label">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Importing...</property>
      </object>
    </child>
    <child>
      <object class="GtkLabel" id="finished_label">
        <property name="visible">True</property>
        <property name="label" translatable="yes">Finished!</property>
      </object>
      <packing>
        <property name="page_type">confirm</property>
      </packing>
    </child>
  </object>
</interface>
